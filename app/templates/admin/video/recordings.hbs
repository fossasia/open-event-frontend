  <Tables::Default
    @columns={{this.columns}}
    @rows={{this.model.data}}
    @currentPage={{this.page}}
    @hideSearchBox={{true}}
    @pageSize={{this.per_page}}
    @sortBy={{this.sort_by}}
    @metaData={{this.model.meta}}
    @widthConstraint="eq-container"
    @resizeMode="fluid"
    @fillMode="equal-column" />