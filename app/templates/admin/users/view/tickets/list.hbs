<div class="row">
  <div class="sixteen wide column">
    {{#each model.orders as |order|}}
      {{#order-card order=order}}
      {{/order-card}}
      <div class="ui hidden divider"></div>
    {{else}}
      <div class="ui disabled header">{{t 'No tickets found for this user'}}</div>
    {{/each}}
  </div>
</div>
