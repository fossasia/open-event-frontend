<div class="ui grid stackable">
  <div class="row">
    <div class="sixteen wide column">
      {{#tabbed-navigation isNonPointing=true}}
        {{#link-to 'admin.orders.list' 'completed' class='item'}}
          {{t 'Completed'}}
        {{/link-to}}
        {{#link-to 'admin.orders.list' 'placed' class='item'}}
          {{t 'Placed'}}
        {{/link-to}}
        {{#link-to 'admin.orders.list' 'pending' class='item'}}
          {{t 'Pending'}}
        {{/link-to}}
        {{#link-to 'admin.orders.list' 'cancelled' class='item'}}
          {{t 'Cancelled'}}
        {{/link-to}}
        {{#link-to 'admin.orders.list' 'expired' class='item'}}
          {{t 'Expired'}}
        {{/link-to}}
        {{#link-to 'admin.orders.list' 'deleted' class='item'}}
          {{t 'Deleted'}}
        {{/link-to}}
        {{#link-to 'admin.orders.list' 'all' class='item'}}
          {{t 'All'}}
        {{/link-to}}
      {{/tabbed-navigation}}
    </div>
  </div>
  <div class="row">
    {{outlet}}
  </div>
</div>
