<div class="sixteen wide column session-list {{if (lte this.model.sessions.data.length 1) 'reset-overflow'}}">
  <Tables::Default
    @columns={{this.columns}}
    @rows={{this.model.sessions.data}}
    @feedbacks={{this.model.feedbacks}}
    @currentPage={{this.page}}
    @pageSize={{this.per_page}}
    @searchQuery={{this.search}}
    @sortBy={{this.sort_by}}
    @sortDir={{this.sort_dir}}
    @metaData={{this.model.sessions.meta}}
    @filterOptions={{this.filterOptions}}
    @widthConstraint="eq-container"
    @resizeMode="fluid"
    @fillMode="equal-column" />
</div>
