{{#if @record.url}}
  <div>
    {{#if (eq @record.videoChannel.provider 'bbb')}}
      {{t 'Auto-Generated'}}
    {{else}}
    <div class="ui input" style="width: 100%; height: 36px">
      <input class="truncate" type="text" value={{this.link}} placeholder={{this.link}} readonly>
    </div>
    {{/if}}
  </div>
{{/if}}
