{{#if @record.url}}
  <div>
    {{#if (eq @record.videoChannel.provider 'bbb')}}
      {{t 'Auto-Generated'}}
    {{else}}
    <div class="ui input" style="width: 100%; height: 36px">
      <input class="truncate" type="text" value={{@record.url}} placeholder={{@record.url}} readonly>
    </div>
    {{/if}}
  </div>
{{/if}}
